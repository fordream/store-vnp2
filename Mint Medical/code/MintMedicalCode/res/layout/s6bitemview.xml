<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/id001_s6item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:visibility="gone" >

        <vnp.mr.mintmedical.view.BaseTextViewBold
            android:id="@+id/s6bitem__textView1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dip"
            android:layout_marginTop="15dp"
            android:gravity="center|left"
            android:text="TextView"
            android:textColor="#333333"
            android:textStyle="bold" />

        <LinearLayout
            android:id="@+id/s11spac"
            android:layout_width="match_parent"
            android:layout_height="1dip"
            android:layout_marginBottom="8dip"
            android:background="@color/listview_spac"
            android:orientation="horizontal" >
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/s6bitem_content1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center" >

        <CheckBox
            android:id="@+id/s6bcheckbok"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/s6b_check"
            android:button="@null" />

        <vnp.mr.mintmedical.view.BaseTextView
            android:id="@+id/s6bitemtext"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:paddingLeft="20dp"
            android:singleLine="true"
            android:text="TextView"
            android:textColor="#333333" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/s6bitem_content1_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:visibility="gone" >

        <vnp.mr.mintmedical.view.BaseTextViewBold
            android:id="@+id/s6bitemtext_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="TextView"
            android:textColor="#999999" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/s6bcontent"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="200dp"
        android:gravity="center|top"
        android:orientation="vertical"
        android:visibility="gone" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center|top"
            android:orientation="vertical" >

            <vnp.mr.mintmedical.view.BaseTextViewBold
                android:id="@+id/s6bactivity__textView1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="Enter other medications"
                android:textColor="#333333"
                android:textStyle="bold" />

            <vnp.mr.mintmedical.view.BaseEditTextView
                android:id="@+id/s6btextview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="15dip"
                android:layout_marginTop="5dip"
                android:background="@drawable/s6_edt"
                android:gravity="top"
                android:minLines="5"
                android:padding="5dip" />

            <Button
                android:id="@+id/s6bbutton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/s5_button" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>